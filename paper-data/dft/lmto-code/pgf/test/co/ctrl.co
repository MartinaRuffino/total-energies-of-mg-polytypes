%const  nit=30 ldau=0 met=ldau?3:1 rel=1 so=0 nclead=0
HEADER  fcc Co, conductance using layered GF
VERS    LMASA-6 LM:7 ASA:7
IO      SHOW=f HELP=f VERBOS=31,20 WKP=F IACTIV=F
CONST   a=6.758 nk=16
#STRUC   NBAS=1 NSPEC=1 NL=3 TET=1 ALAT=a
        PLAT=0.0 0.5 0.5 0.5 0.0 0.5 0.5 0.5 0.0
STRUC   NBAS=4 NSPEC=2 NL=3 TET=1 ALAT=a
        PLAT=0.0 0.5 0.5
             0.0 0.5 -.5
             2.0 0.0 0.0
SYMGRP  r4x find
%const  pgf=1 sparse=0
%const  gfmod=0
PGF     MODE={pgf} SPARSE={sparse} PLATL=1 0 0 PLATR=1 0 0
        GFOPTS=p3;{?;nclead;nclead;}
GF      MODE={gfmod} GFOPTS=p3
SPEC    ATOM=Co Z=27 R=2.641002 IDXDN=1 1 1 A=.03
        IDU= 0 0 {ldau?2:0}  UH= 0 0 0.1 JH= 0 0 0
        ATOM=Cob Z=27 R=2.641002 IDXDN=1 1 1 A=.03
        IDU= 0 0 {ldau?2:0}  UH= 0 0 0.1 JH= 0 0 0
SPEC    ATOM=Co Z=27 R=2.641002 IDXDN=1 1 1 A=.01
        IDU= 0 0 {ldau?2:0}  UH= 0 0 0.1 JH= 0 0 0
        ATOM=Cob Z=27 R=2.641002 IDXDN=1 1 1 A=.01
        IDU= 0 0 {ldau?2:0}  UH= 0 0 0.1 JH= 0 0 0
SITE    ATOM=Co POS=0.0 0.0 0.0 PL=0
        ATOM=Cob POS=0.5 0.0 0.5 PL=0
        ATOM=Co POS=1.0 0.0 0.0 PL=1
        ATOM=Cob POS=1.5 0.0 0.5 PL=1
OPTIONS NSPIN=2 REL=T HF=0 ASA[ CCOR=F TWOC=T GAMMA=T ]
% const udiag=t
HAM     UDIAG={udiag} NSPIN=2 REL=T QASA=0 REL={rel} SO={so|rel==2} 
%const  ef=-0.0595884
BZ      NKABC=nk nk 1 BZJOB=0 NPTS=1001 EFMAX=1 DOS=-1 0.6
        EF0=0 DELEF=.1 TETRA=1 METAL=1 INVIT=T NEVMX=0
% if pgf==1
        EMESH=20 10 -1.0 {ef} .5 .5
% endif
% if pgf==2
        EMESH=20 10 -1.0 {ef} .5 .5
% endif
% if pgf==3
        EMESH=400 0 -.8 1 0
% endif
% if pgf==5
        EMESH=201 1 -0.8 0.147 0.00001
% endif
EWALD   TOL=1d-12 NKDMX=2000
ITER    MIX=A3,b=0.1 NIT={abs(nit)} CONVC=1d-5 CONV=0
MIX     MODE=A3,b=0.1
START   BEGMOM={nit<0?2:nit==0} CNTROL={nit==0}
