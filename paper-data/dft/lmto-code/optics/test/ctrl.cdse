% const nsp=2 so=t
HEADER  wurtzite CdSe
VERS    LMASA-6.10
TEST    lmstr cdse >/dev/null
        echo set up potential
        lm cdse -vnsp=2 -vnit=0 -vso=1
        lm cdse
IO      SHOW=f HELP=f VERBOS=31,20 WKP=F IACTIV=F
OPTIONS NSPIN={nsp} REL=t CCOR=T LMH=F ADNF=F Q= FRZ=f NONCOL={so} SO={so}
%ifdef so
OPTICS  MODE=1 NPTS=500 WINDOW=0 1 FILBND=23 36 EMPBND=37 50 PART=f
%else
OPTICS  MODE=1 NPTS=500 WINDOW=0 1 FILBND=12 18 EMPBND=19 25 PART=f
%endif
CMD     --sfill=1,2,3,4
SYMGRP  R6Z:T(0,-sqrt(3)/3,c/2) MX
BZ      METAL=t TETRA=t GETQP=f DOSWT=t NKABC=12 12 10
        N=0 W=0.05  TOL=1E-5 NEVMX=71 EFMAX=3.  BZJOB=f DRNGE=-1.1 1.      
STR     RMAX=3.05
CONST   c=7.011/4.3 a=4.3/0.529 u=0.367 v=0.5+u nit=1
STRUC   NBAS=8 NCLASS=4 NL=3
        ALAT=a PLAT=  1 0 0 -.5 .8660254 0 0 0 c
SITE    ATOM=Cd POS=  0   0   0
        ATOM=Cd POS=  0.5  0.2886751 c/2
        ATOM=Se POS=  0   0  u*c
        ATOM=Se POS=  0.5 0.2886751 v*c
        ATOM=E1 POS=  0 0 0.6875*c
        ATOM=E1 POS=  0.5 0.2886751 0.1875*c
        ATOM=E2 POS=  0.0 0.5773502 0.1875*c
        ATOM=E2 POS=  0.0 0.5773502 0.6875*c
CLASS   ATOM=Cd Z=48  R/W=1.00
        ATOM=Se Z=34  R/W=1.00
        ATOM=E1 Z=0   R/W=0.75 
        ATOM=E2 Z=0   R/W=1.16 
MIX     MODE=b
START   NIT=nit CNVG=1E-5 FREE=f BEGMOM=1 CNTROL=(nit==0)
