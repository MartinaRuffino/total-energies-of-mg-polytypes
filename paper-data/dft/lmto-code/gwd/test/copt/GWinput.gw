    1    4    2    0    0   ! 5g_d  
    2    0    1    1    1   ! 6S_p *
    2    0    2    0    0   ! 6S_d  
    2    1    1    1    1   ! 6P_p  
    2    1    2    0    0   ! 6P_d  
    2    2    1    1    1   ! 5D_p  
    2    2    2    0    0   ! 5D_d  
    2    2    3    0    1   ! 6D_l  
    2    3    1    0    1   ! 5F_p  
    2    3    2    0    0   ! 5F_d  
    2    4    1    0    0   ! 5g_p  
    2    4    2    0    0   ! 5g_d  
  atom   l    n  occ unocc   ForX0 ForSxc :CoreState(1=yes, 0=no)
    1    0    1    0    0      0    0    ! 1S *
    1    0    2    0    0      0    0    ! 2S  
    1    0    3    1    0      0    0    ! 3S  
    1    1    1    0    0      0    0    ! 2P  
    1    1    2    1    0      1    1    ! 3P  
    2    0    1    0    0      0    0    ! 1S *
    2    0    2    0    0      0    0    ! 2S  
    2    0    3    0    0      0    0    ! 3S  
    2    0    4    0    0      0    0    ! 4S  
    2    0    5    1    0      0    0    ! 5S  
    2    1    1    0    0      0    0    ! 2P  
    2    1    2    0    0      0    0    ! 3P  
    2    1    3    0    0      0    0    ! 4P  
    2    1    4    1    0      1    1    ! 5P  
    2    2    1    0    0      0    0    ! 3D  
    2    2    2    0    0      0    0    ! 4D  
    2    3    1    1    0      0    0    ! 4F  
</PRODUCT_BASIS>

