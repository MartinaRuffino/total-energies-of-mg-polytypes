echo --- slatsm-to-awrite: ccomp for AWRITE ---

echo '   ' slatsm-to-awrite: ccomp for AWRITE files: \
     `egrep -l '^[Cc]#(ifdef[Cc]|elseif[Cc]).*AWRITE' *.f`
foreach i ( `egrep -l '^[Cc]#(ifdef[Cc]|elseif[Cc]).*AWRITE' *.f` )
  rm -f xx.f ; ccomp -uAWRITE $i xx.f
  ccomp -dAWRITE xx.f $i
end

rm -f xx.f
