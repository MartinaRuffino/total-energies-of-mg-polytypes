echo --- slatsm-to-apollo: ccomp to APOLLO-specific ---

rm -f xx.f ; ccomp -uAPOLLO cpudel.f xx.f
ccomp -uAIX -dAPOLLO -uCONVEX -uCRAY -uDEC -uHP -uLINUX -uSGI -uSUN xx.f cpudel.f

rm -f xx.f
foreach i ( ` grep -l APOLLO *.f `)
  rm -f xx.f ; ccomp -uAPOLLO $i xx.f
  ccomp -dAPOLLO xx.f $i
end

rm -f xx.f ; ccomp -uFORMATTED_READ a2bin.f xx.f
ccomp -dFORMATTED_READ -dAPOLLO_BUG xx.f a2bin.f

#foreach i (yhmul.f zampy.f zhmpy.f)
#  rm -f xx.f ; ccomp -uBLAS $i xx.f
#  ccomp -dBLAS xx.f $i
#end

rm -f xx.f
