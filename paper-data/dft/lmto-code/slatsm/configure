ac_subst_vars='SHELL PATH_SEPARATOR PACKAGE_NAME PACKAGE_TARNAME PACKAGE_VERSION PACKAGE_STRING PACKAGE_BUGREPORT exec_prefix prefix program_transform_name bindir sbindir libexecdir datadir sysconfdir sharedstatedir localstatedir libdir includedir oldincludedir infodir mandir build_alias host_alias target_alias DEFS ECHO_C ECHO_N ECHO_T LIBS INSTALL_PROGRAM INSTALL_SCRIPT INSTALL_DATA LN_S RANLIB ac_ct_RANLIB SET_MAKE build build_cpu build_vendor build_os host host_cpu host_vendor host_os target target_cpu target_vendor target_os LIBSLA LIBSLA_MPI LIBLOC_MPI SLDIR CC CCC FC FCOMP CCtry CFLAGS LDFLAGS CPPFLAGS ac_ct_CC EXEEXT OBJEXT FFLAGS ac_ct_FC FLIBS CMAIN FC_UPPERCASE FC_UNDERSCORE NOPASSEDARGS NARGFCALL ADD_TO_NARGFCALL ARGFCALL CCOMP CCOMPDIR FC_IS_F90 FC_AUTOARRAY FC_POINTER FC_RECUR FC_VSTRINGS CCOMP_SW ADD0 POSZER LIBLOC srcdir ISCRIPT F90M LIBOBJS LTLIBOBJS'
ac_subst_files=''

