echo -------- ccomp to DEC-specific -------

#rm -f xx.f ; ccomp -uRIEEE d1mach.f xx.f
#ccomp -uIEEE -dRIEEE -uCRAY xx.f d1mach.f

rm -f xx.f
foreach i (poseof.f)
  rm -f xx.f ; ccomp -uDEC $i xx.f
  ccomp -dDEC xx.f $i
end

rm -f xx.f ; ccomp -uDEC cpudel.f xx.f
ccomp -uAIX -uAPOLLO -uCONVEX -uCRAY -dDEC -uHP -uLINUX -uSGI -uSUN xx.f cpudel.f

rm -f xx.f
foreach i ( ` grep -l BLAS3 *.f `)
  rm -f xx.f ; ccomp -uBLAS3 $i xx.f
  ccomp -dBLAS3 xx.f $i
end

#foreach i ( {hcinv,yhchd,yhmul,zampy,zhmpy,dmpy,dampy,yaxcpy,yaxpy,dpcopy,dpscop,htribk,htridi}.f )
#  rm -f xx.f ; ccomp -uBLAS $i xx.f
#  ccomp -dBLAS xx.f $i
#end

foreach i (diagno.f zhev.f)
  rm -f xx.f ; ccomp -uSGI $i xx.f
  ccomp -dSGI xx.f $i
end

rm -f xx.f
