% ifdef ldau
        IDU= 0 0 2 2 UH= 0 0 0.1 0.632 JH= 0 0 0 0.055
% endif
        ATOM=As  Z=33 R/W=1 LMX=2 RSMH= 1.4 1.4 1.4  EH= -0.6 -0.1 -0.1
        EREF=-4515.471448  IDMOD=0 0 1
% const idx=1
        ATOM=EA1  Z=0  R/W=1 IDXDN=1 1 {idx} IDMOD=0 0 1 LMX=2
        ATOM=EC1  Z=0  R/W=1 IDXDN=1 1 {idx} IDMOD=0 0 1 LMX=2
