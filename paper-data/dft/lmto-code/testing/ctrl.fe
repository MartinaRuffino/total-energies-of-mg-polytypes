% const lxcf=2 lxcg=f nsp=2 nl=3 ccor=t nit=1 nk=6 novmza=f
% const twoc=f mode=twoc?10:0 tetra=t bzj=0 cond=0 mdos=f
% ifdef lxcf==1
% const eref=lxcg?-2545.8752:-2540.6621
% elseifd lxcf==2
% const eref=lxcg?-2542.34265:-2540.56813
% elseifd lxcf==3
% const eref=lxcg==3?-2545.1730:-2540.6402
% endif
HEADER  Study of iron
TESTDOS lmstr fe >/dev/null
        lm -vnit=1 -vnk=6 fe --no-iactiv
        lmdos --dos:npts=1001:window=-.7,.8 -vnk=6 fe
TESTDSM lmstr fe >/dev/null
        lm -vnit=1 -vnk=6 fe --no-iactiv -vmdos=t 
        lmdos --dos:npts=501:window=-.8,.5 fe -vmdos=t 
TESTCND lmdos --dos:mode=1:npts=501:window=-.8,.5:vec=0,0,1 fe
TSTJDOS lmstr fe
        lm fe -vnk=12 --no-iactiv -vlteto=2 -voptmod=-1 --quit=band
TSTJDS3 lmstr fe
        lm fe -vnk=12 --no-iactiv -vlteto=3 -voptmod=-1 --quit=band
TSTMULL lmstr fe >/dev/null
        lm -vnit=1 -vnk=6 --mull:mode=1 -vmetal=2 fe --iactiv=no
        lmdos --mull:mode=1 --dos:npts=1001:window=-.7,.8 -vnk=6 fe
# Levenberg-Marquardt: copy {vext,refbnds,rsta,syml}.fe in this directory before running test
# rdcmd '-f:#rdcmd:%2f' -cat:TESTLM --noerr ctrl.fe > out.fe
# Compare the following files:
# diff vext0.fe testing
# zdiff out.fe testing/out.lmarq.fe.gz
TESTLM  lm --rs=1,0 -vnit=5 -vnk=12 fe --iactiv=no -vrdvext=2 -vlmfit=2 -vccor=f
        cp vext0.fe vext.fe
        lm --rs=1,0 -vnit=5 -vnk=12 fe --iactiv=no -vrdvext=2 -vlmfit=0 -vccor=f --quit=band
        lm --rs=1,0 -vnit=5 -vnk=12 fe --iactiv=no -vrdvext=2 -vlmfit=0 -vccor=f --band:fn=syml
# This is the MPIK version .... doesn't work yet ...
TESTLM  asa-soft-link-str fe
        mpirun -n 8 lm --rs=1,0 -vnit=5 -vnk=12 fe --iactiv=no -vrdvext=1 -vlmfit=1 -vccor=f -vmetal=2
        cp vext0.fe vext.fe
        lm --rs=1,0 -vnit=5 -vnk=12 fe --iactiv=no -vrdvext=1 -vlmfit=0 -vccor=f --quit=band
        lm --rs=1,0 -vnit=5 -vnk=12 fe --iactiv=no -vrdvext=1 -vlmfit=0 -vccor=f --band:fn=syml
VERS    LMASA-6 LMF-6 LM:7 ASA:7
IO      SHOW=F HELP=f VERBOS=31 20 WKP=F IACTIV=t
# for Version 7:
% const rdvext=0 lmfit=0 sharm=0
HAM     NSPIN={nsp} QASA=0 NONCOL=f GGA={lxcg} XCFUN={lxcf} RDVEXT={rdvext}
OPTIONS NSPIN={nsp} LXCG={lxcg} ASA[ ADNF=T CCOR={ccor} TWOC={twoc} ]
        NONCOL=f SHARM={sharm}
% const optmod=0 lteto=3
OPTICS  MODE=optmod NPTS=301 WINDOW=0 1 ESCISS=0 LTET={lteto}
CONST   ns=.8
% const metal=1
BZ      NKABC={nk} SAVDOS={mdos?4:0} NPTS=1001 NEVMX=18 EFMAX=9 PUTQP=f
        TETRA={tetra} COND:{cond},0,0,1 BZJOB={bzj} NOINV=f INVIT=F FSMOM!=0
        METAL={metal}
STR     RMAX=3.5 MODE={mode}
% ifdef dble
CLASS   ATOM=FE  Z=26 R/W=1 IDXDN=0 0 0 2 EREF={eref} A=.012
        ATOM=FE2 Z=26 R/W=1 IDXDN=0 0 0 2 EREF={eref} A=.012
STRUC   NBAS=2 NSPEC=2 NL={nl}
        ALAT=5.406 PLAT= 1 0 0  0 1 0  0 0 1
% ifdef tet
        SHEAR=0 0 1 {tet}
% elseifd trig
        SHEAR=1 1 1 {trig}
SYMGRP  i*r3d r2(1,0,-1)
% endif
SITE    ATOM=FE POS= 0 0 0  ATOM=FE2 POS= .5 .5 .5
% else
CLASS   ATOM=FE Z=26 R/W=1 IDXDN=0 0 0 2 EREF={eref} A=.012 MOM=0,0,2.2
STRUC   NBAS=1 NSPEC=1 NL={nl}
        ALAT=5.406 PLAT=  -.5 .5 .5  .5 -.5 .5  .5 .5 -.5
% ifdef tet
        SHEAR=0 0 1 {tet}
% elseifd trig
        SHEAR=1 1 1 {trig}
SYMGRP  i*r3d r2(1,0,-1)
% endif
%ifdef mdos
SYMGRP  I*I
%endif
SYMGRP  R4X MX R3D
SITE    ATOM=FE POS= 0 0 0
% endif
.MIX     MODE=B8,b=.50,w=2,1n=4;B4,w=1,2,b=.5,k=4
# for Version 7:
ITER     MIX=B NIT={nit} CONVC=1D-7 CONV=0
         FIT[MODE={lmfit} NBFIT=1 8 NBFITF=1 LAM=1 SCL=5 SHFT=1 WT=1,.1,.08]
MIX     MODE=B
#START   CNTROL={nit==0} BEGMOM={nit==0}
START   CNTROL=1 BEGMOM=1
