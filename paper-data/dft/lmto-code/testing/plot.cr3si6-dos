%char0 ltdos="1,bold=3,col=0,0,0"
%char0 ltdos2="3,bold=4,col=1,0,0"

fplot

% var ymax=1.4 dy=0.062 dw=.00 ymax+=dy emin=-0.8 emax=0.5 ef=0

% var ymax-=dy+dw dy=0.062 dmin=0 dmax=10
