 -colsy 2 -lt {ltdos} -qr dosp.asa
 -colsy 2 -lt {ltdos2} -qr dosp.fp
 -lt 2,bold=3,col=0,0,0,2,.5,.05,.5 -tp 2~{ef},{dmin},{ef},{dmax}

