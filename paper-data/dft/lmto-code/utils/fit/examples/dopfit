echo ' -------------- Cubic polynomial ---------------'
pfit 3 pfit.1 <<END
3

END
echo ' '
echo ' ... print out table of points using pfit -tp 1~1:3:.5,3.2 3 pfit.1'
pfit -tp 1~1:3:.5,3.2 3 pfit.1
echo ' -------------- Binomial in two variables ---------------'
cat pfit.2 | pfit 2

echo ' -------------- Minimize quartic in two variables ---------------'
rdfile pfit.4 | pfit -p43 4

echo ' -------------- Finite minimum of quartic in three variables ---------------'
rdfile pfit.icos | pfit 2
