echo "... dotestsi2c: make sigma(k) from 2C H (file out)"
rm *.si; cp sx/testv2/ctrl.si .; lmstr si >/dev/null
lm -vnk=4 -vnit=0 si >out
lm -vnk=4 -vnit=1 -vtim=f si -pr80 >>out <<END








END
echo ----------- compare out with sx/testv2/out.2c -----------
diff out sx/testv2/out.2c | grep -v cpudel

echo ----------- compare out.si with sx/testv2/last.ec -----------
mcx out.si sx/testv2/last.ec -- | grep -v 000000
