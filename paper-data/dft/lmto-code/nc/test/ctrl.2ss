HEADER Test of spin spiral and noncollinear magnetism, 2 atoms/cell
# The following are AFM: +- (all equivalent; permute atom files).
.% const p1=1 p2=-1 nc=f qss=0 thss=0
.% const p1=0 p2=1 nc=t qss=0 thss=0
# The following are +--+, ++--, +--+, +--+ (permute atom files).
.% const p1=1 p2=-1 nc=f qss=1/2 thss=pi/2
.% const p1=1 p2=1 nc=f qss=1/2 thss=pi/2
.% const p1=0 p2=1 nc=t qss=1/2 thss=pi/2
.% const p1=0 p2=1 nc=t qss=1/2 thss=0
# the following does: spins + -> - <-
.% const p1=0 p2=1/2 nc=t qss=1/2 thss=0
# the following is + <- - <-
% const p1=0 p2=1/2 nc=t qss=1/2 thss=pi/2
% const nit=1 ccor=t twoc=f nk=4 nc=f qss=1/2 thss=pi/2
% echo ccor={ccor} nc={nc} twoc={twoc} qss={qss} spin = {p1} {p2}
% if nc
CLASS   ATOM=A1 Z=26 EREF=eref A=.01 NR=901 R/W=1 GRP2=1
        ATOM=A2 Z=26 EREF=eref A=.01 NR=901 R/W=1 GRP2=1
% else
CLASS   ATOM=A1 Z=26 EREF=eref A=.01 NR=901 R/W=1 GRP2={p1}
        ATOM=A2 Z=26 EREF=eref A=.01 NR=901 R/W=1 GRP2={p2}
% endif
SYMGRP  R4Z MX
% if thss==0
SITE    ATOM=A1 POS=   0   0   0    ROT=z:{p1}*pi,y:pi/2
        ATOM=A2 POS=  .5     0 .5   ROT=z:{p2}*pi,y:pi/2
% else
SITE    ATOM=A1 POS=   0   0   0    ROT=y:{p1}*pi
        ATOM=A2 POS=  .5     0 .5   ROT=y:{p2}*pi
% endif
STRUC   NBAS=2 NCLASS=2 ALAT=a PLAT=.5 .5 0  .5 -.5 0  0 0 1
VERS    LMFP-4.0 LMASA-6.1 LM:7 ASA:7
STR     RMAX=2.7 ENV[ MODE=10 ]
CONST   a=7.113672904 rc=0 tet=1 bzj=1 mom=2.2 eref=-2540.5681
IO      SHOW=f HELP=F VERBOS=31 20 WKP=F IACTIV=t
HAM     NSPIN=2 NONCOL={nc} QASA=0 SS=0 0 {qss} {thss}
OPTIONS NONCOL={nc} ASA[ CCOR={ccor} TWOC={twoc} ELIN=-.2 ]
        NSPIN=2 INVIT=t SS=0 0 {qss} {thss}
% const tetra=t metal=2
BZ      NKABC={nk} {nk} {nk} BZJOB=bzj TETRA={tetra} METAL={metal}
EWALD   AS=2.0 TOL=1D-6 ALAT0=a
ITER    MIX=B,w=1,2,b=.4,n=3;B,b=.02,w=2,1,n=3 NIT={nit} CONVC=1D-7 CONVC=0
MIX     MODE=B,w=1,2,b=.4,n=3;B,b=.02,w=2,1,n=3
ITER    MIX=B,w=1,1,b=.02 NIT={nit}
MIX     MODE=B,w=1,1,b=.02
ITER    MIX=B,w=3,1,b=.02,n=4;B,b=.02,w=1,1,n=2 NIT={nit}
MIX     MODE=B,w=3,1,b=.02,n=4;B,b=.02,w=1,1,n=2
START   CNTROL={nit==0} BEGMOM={nit==0} NIT={nit} CNVG=1D-7
