%const  nit=30
HEADER  fcc Co, conductance using layered GF
VERS    LMASA-6 LM:7 ASA:7
IO      SHOW=f HELP=f VERBOS=31,20 WKP=F IACTIV=F
GF      MODE=1 GFOPTS=emom
STR     RMAXS=3.8 
CONST   a=6.758 nk=16 
STRUC   NBAS=1 NSPEC=1 NL=3 TET=1 ALAT=a 
        ALAT=a  PLAT= 0 .5 .5
	             .5  0 .5
	             .5 .5  0 
SYMGRP  find
SPEC    ATOM=A Z=27 R=2.641002 IDXDN=1 1 1 IDMOD=0 0 0 A=0.03
SITE    ATOM=A POS=0.0 0.0 0.0
HAM      NSPIN=2 REL={rel} SO=T QASA=0
OPTIONS NSPIN=2 REL={rel} SO=T ASA[ CCOR=F TWOC=T GAMMA=T ]
%const  ef=-0.059545
BZ      NKABC=nk nk nk BZJOB=0 NPTS=1001 DOS=-1 0.6 
        TETRA=1 METAL=1 INVIT=T SAVDOS=T
% if pgf==1
        EMESH=12 10 {ef-1.2} {ef} .5 .0
% endif
% if pgf==2
        EMESH=20 10 -1.0 {ef} .5 .5
% endif
% if pgf==5
        EMESH=201 1 -0.8 0.147 0.00001
% endif
ITER    MIX=A3,b=0.1 NIT={nit} CONVC=1d-5 CONV=0
MIX     MODE=A3,b=0.1
START   NIT={nit} CNVG=1d-5 BEGMOM=T CNTROL=F
