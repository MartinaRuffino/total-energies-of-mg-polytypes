SYMGRP  i*r3(1,1,-1) r4x SOC=1
HEADER  rocksalt GdN
TESTLMF lmfa gdn
        lmf  gdn 
        mv rst.gdn rst-save.gdn
        mv dmats.gdn dmats-save.gdn
        rm -f rst.gdn wkp.gdn mixm.gdn
        echo make bands with f block rendered diagonal
        rm -f rst.gdn wkp.gdn mixm.gdn
        cp rst-save.gdn rst.gdn
        cp dmats-save.gdn dmats.gdn
        lmf --zhblock~e=-1.5,1.4~10:16 gdn --rs=1,0 -vnit=1
        cp rst-save.gdn rst.gdn
        cp dmats-save.gdn dmats.gdn
        lmf --zhblock~e=-1.5,1.4~10:16 gdn --rs=1,0 --band:fn=syml
        mv bnds.gdn bnds.fdiagonal.gdn
        echo make bands without B field ...
        cp rst-save.gdn rst.gdn
        cp dmats-save.gdn dmats.gdn
        rm -f mixm.gdn
        lmf  gdn --rs=1,0 -vnit=1 -vso=t --eveca
        lmf  gdn --rs=1,0 -vnit=1 -vso=t --band:fn=syml
        mv bnds.gdn bnds.nobfield.gdn
        echo make bands with B field ...
        cp rst-save.gdn rst.gdn
        cp dmats-save.gdn dmats.gdn
        rm -f mixm.gdn
        lmf  gdn --rs=1,0 -vnit=1 -vso=t -vbf=2
        lmf  gdn --rs=1,0 -vnit=1 -vso=t -vbf=2 --band:fn=syml
        mv bnds.gdn bnds.bfield.gdn
        echo LDA+U calculation with PWs added to basis ...
        cp rst-save.gdn rst.gdn
        cp dmats-save.gdn dmats.gdn
        rm -f mixm.gdn
        lmf  gdn --rs=1,0 -vpwmode=1
        rm -f rst.gdn wkp.gdn mixm.gdn
        lmfa gdn -vcoref=t
        lmf  gdn -vcoref=t 
MPIK version:
TMPKLMF lmfa gdn
        mpirun -n 4 lmf gdn
        mv rst.gdn rst-save.gdn
        mv dmats.gdn dmats-save.gdn
        rm -f rst.gdn wkp.gdn mixm.gdn
        echo make bands with f block rendered diagonal
        rm -f rst.gdn wkp.gdn mixm.gdn
        cp rst-save.gdn rst.gdn
        cp dmats-save.gdn dmats.gdn
        mpirun -n 4 lmf --zhblock~e=-1.5,1.4~10:16 gdn --rs=1,0 -vnit=1
        cp rst-save.gdn rst.gdn
        cp dmats-save.gdn dmats.gdn
        mpirun -n 4 lmf --zhblock~e=-1.5,1.4~10:16 gdn --rs=1,0 --band:fn=syml
        mv bnds.gdn bnds.fdiagonal.gdn
        echo make bands without B field ...
        cp rst-save.gdn rst.gdn
        cp dmats-save.gdn dmats.gdn
        rm -f mixm.gdn
        mpirun -n 4 lmf  gdn --rs=1,0 -vnit=1 -vso=t --eveca
        mpirun -n 4 lmf  gdn --rs=1,0 -vnit=1 -vso=t --band:fn=syml
        mv bnds.gdn bnds.nobfield.gdn
        echo make bands with B field ...
        cp rst-save.gdn rst.gdn
        cp dmats-save.gdn dmats.gdn
        rm -f mixm.gdn
        mpirun -n 4 lmf  gdn --rs=1,0 -vnit=1 -vso=t -vbf=2
        mpirun -n 4 lmf  gdn --rs=1,0 -vnit=1 -vso=t -vbf=2 --band:fn=syml
        mv bnds.gdn bnds.bfield.gdn
        echo LDA+U calculation with PWs added to basis ...
        cp rst-save.gdn rst.gdn
        cp dmats-save.gdn dmats.gdn
        rm -f mixm.gdn
        mpirun -n 4 lmf  gdn --rs=1,0 -vpwmode=1
        rm -f rst.gdn wkp.gdn mixm.gdn
        lmfa gdn -vcoref=t
        mpirun -n 4 lmf  gdn -vcoref=t 
TESTCLS cp rst-save.gdn rst.gdn
        cp dmats-save.gdn dmats.gdn
        lmf gdn -vmet=3 --mull:mode=1 -vnk=6 -vnit=1 -vso=t
        mv dos.gdn tdos.gdn
        lmdos -vso=t --mull:mode=1 --dos:fpdos:npts=1001:window=-1,1 -vnk=6 gdn
        mv dos.gdn dos-mull.gdn
        lmf gdn --mull:mode=2 -vnk=6 -vnit=1 -vso=t --rs=1,0 --nosym
        lmdos -vso=t --mull:mode=2 --dos:rdm:fpdos:npts=501:window=-.5,.5 -vnk=6 gdn --nosym
        cp dos.gdn dos.nosym.gdn
        lmf gdn --mull:mode=2 -vnk=6 -vnit=1 -vso=t --rs=1,0 
        cp dos.gdn dos.s.gdn
        echo -.5 .5 / | pldos -fplot -lst=1 dos.gdn 
        mcx dosp.dat -inc 'x1>=-.5&x1<=0.5' > tdos1.gdn
        lmdos -vso=t --mull:mode=2 --dos:rdm:fpdos:npts=501:window=-.5,.5 -vnk=6 gdn 
        mcx dos.gdn -p -coll 1 -tog -coll 2:nc -csum -ccat > tdos.sym

PLOTMUL echo 150 10 -9 10 | pldos -ef=0 -escl=13.6 -fplot '-lst=7;8' -ref:fn=tdos.gdn:chan=1:scale dos-mull.gdn
