HEADER  GaN in the ZB structure
TESTLMF rm -f mixm.zbgan opt-tet3.zbgan
        lmfa zbgan
        lmf zbgan --rs=0,1 -vnit=10
        lmf zbgan --rs=1,0 -vnit=1 -vloptic=-1 -vlteto=0
        mv jdos.zbgan jdos-samp.zbgan
        lmf zbgan --rs=1,0 -vnit=1 -vloptic=-1 -vlteto=3
        mv jdos.zbgan jdos-tet3.zbgan
        mv popt.zbgan pjdos-tet3.zbgan
        lmf zbgan --rs=1,0 -vnit=1 -vloptic=-1 -vlteto=3 -vlpart=2
        mv popt.zbgan pjdos-tetk.zbgan
        lmf zbgan --rs=1,0 -vnit=1 -vloptic=1 -vlteto=3 -vnk=20
        mv opt.zbgan opt-tet3.zbgan
        lmf zbgan --rs=1,0,0,1 -vnit=1 -vloptic=1 -vlteto=0 -vnk=20 
        mv opt.zbgan opt-samp.zbgan
        lmf zbgan --rs=1,0,0,1 -vnit=1 -vloptic=8 -vlteto=0 -vnk=60
        mv opt.zbgan opt-abs.zbgan
        lmf zbgan --rs=1,0,0,1 -vnit=1 -vloptic=9 -vlteto=0 -vnk=60
        mv opt.zbgan opt-lum.zbgan
MPIK version
TMPKLMF rm -f mixm.zbgan opt-tet3.zbgan
        lmfa zbgan
        mpirun -n 6 lmf zbgan --rs=0,1 -vnit=10
        mpirun -n 6 lmf zbgan --rs=1,0 -vnit=1 -vloptic=-1 -vlteto=0
        mv jdos.zbgan jdos-samp.zbgan
        mpirun -n 6 lmf zbgan --rs=1,0 -vnit=1 -vloptic=-1 -vlteto=3
        mv jdos.zbgan jdos-tet3.zbgan
        mv popt.zbgan pjdos-tet3.zbgan
        mpirun -n 6 lmf zbgan --rs=1,0 -vnit=1 -vloptic=-1 -vlteto=3 -vlpart=2
        mv popt.zbgan pjdos-tetk.zbgan
        mpirun -n 6 lmf zbgan --rs=1,0 -vnit=1 -vloptic=1 -vlteto=3 -vnk=20
        mv opt.zbgan opt-tet3.zbgan
        mpirun -n 6 lmf zbgan --rs=1,0,0,1 -vnit=1 -vloptic=1 -vlteto=0 -vnk=20 
        mv opt.zbgan opt-samp.zbgan
        mpirun -n 6 lmf zbgan --rs=1,0,0,1 -vnit=1 -vloptic=8 -vlteto=0 -vnk=60
        mv opt.zbgan opt-abs.zbgan
        mpirun -n 6 lmf zbgan --rs=1,0,0,1 -vnit=1 -vloptic=9 -vlteto=0 -vnk=60
        mv opt.zbgan opt-lum.zbgan
MPI version
TMPILMF rm -f mixm.zbgan opt-tet3.zbgan
        lmfa zbgan
        mpirun -n 2 lmf-MPI zbgan --rs=0,1 -vnit=10
        mpirun -n 2 lmf-MPI zbgan --rs=1,0 -vnit=1 -vloptic=-1 -vlteto=0
        mv jdos.zbgan jdos-samp.zbgan
        mpirun -n 2 lmf-MPI zbgan --rs=1,0 -vnit=1 -vloptic=-1 -vlteto=3
        mv jdos.zbgan jdos-tet3.zbgan
        mv popt.zbgan pjdos-tet3.zbgan
        mpirun -n 2 lmf-MPI zbgan --rs=1,0 -vnit=1 -vloptic=-1 -vlteto=3 -vlpart=2
        mv popt.zbgan pjdos-tetk.zbgan
        echo optics not implemented with MPI ... test will fail
TSTWDEN rm -f mixm.zbgan smrho.zbgan
        lmfa zbgan
        lmf zbgan --rs=0,1 -vnit=10
        lmf ctrl.zbgan -vgmax=24 --vext~atpos~ro=0,0,.25~l1=1,1,0,41~l2=-1,0,1,41
        lmf ctrl.zbgan -vgmax=24 --wden~q=0,0,0.001~core=-1~ro=0,0,.25~lst=9~l1=-1,1,1,41~l2=1,-1,1,41

CLEAN   rm -f jdos-tet.zbgan jdos.zbgan pjdos-tetk.zbgan jdos-samp.zbgan jdos-tet3.zbgan pjdos-tet3.zbgan
        rm -f a1.zbgan c1.zbgan opt-samp.zbgan opt-tet.zbgan opt-tet3.zbgan optdata.zbgan out.zbgan
        rm -f ctrl.zbgan myrho.zbgan opt-abs.zbgan opt-lum.zbgan smrho.zbgan
---------------- ZB structure ----------------
SYMGRP  R4Z*I MX*I R3D find
STRUC   NBAS=4/1 NSPEC=4/1 NL=5 ELINK=-2
        ALAT=a DALAT=da PLAT= 0 .5 .5    .5 0 .5    .5 .5 0
SITE    ATOM=C1  POS=  0+wto   0+wto   0+wto
        ATOM=A1  POS= .25-wto+kl .25-wto+kl .25-wto+kl
        ATOM=EA1 POS= .50+kl/2   .50+kl/2   .50+kl/2
        ATOM=EC1 POS= .75   .75   .75
---------------- Default categories ----------------
SPEC
