HEADER  GaAs in the ZB structure, with local orbitals
TESTLMF lmfa gas
        rm -f mixm.gas
        lmf gas --rs=0,1 -vnit=10
        echo assemble qp list: dense mesh for small q, coarse mesh for large q
        lmf -vnk=40 gas -vtet=0 --rs=0,0 --putqp --nosym --quit=ham
        lmf gas '--band~qp,inc=q<.1,save=qp1~fn=qpts'
        lmf gas --rs=0,0 -vtet=0 --putqp  --quit=ham
        lmf gas '--band~qp,merge=qp1,save=qp~fn=qpts'
        echo generate bands for this list
        lmf gas --band:qp:fn=qp
MPIK version
TMPKLMF lmfa gas
        rm -f mixm.gas
        mpirun -n 8 lmf gas --rs=0,1 -vnit=10
        echo assemble qp list: dense mesh for small q, coarse mesh for large q
        lmf -vnk=40 gas --rs=0,0 --putqp --nosym --quit=ham
        lmf gas '--band~qp,inc=q<.1,save=qp1~fn=qpts'
        lmf gas --rs=0,0 --putqp  --quit=ham
        lmf gas '--band~qp,merge=qp1,save=qp~fn=qpts'
        echo generate bands for this list
        mpirun -n 8 lmf gas --band:qp:fn=qp
TSTPDOS lmfa gas
        rm -f mixm.gas
        lmf  gas --rs=0,1 -vnit=10
        lmf  gas -vpdos=t --pdos~lcut=2,2,1,1~mode=1 --rs=1,0 -vnit=1
        mv dos.gas tdos.gas
        lmdos gas -vpdos=t --pdos~lcut=2,2,1,1~mode=1 --dos:npts=1001:window=-1.2,.7
        mv dos.gas pdos.gas
PLOTDOS echo 15 7 -1.2 0.7 | pldos -fplot -lst="1;2;3;4;5;6" pdos.gas
lmf  gas -vpdos=t --pdos~nl=3~mode=1 --rs=1,0 -vnit=1
lmdos gas -vpdos=t --pdos~nl=3~mode=1 --dos:npts=1001:window=-1.2,.7
# Compare total to partial dos in the Ga, As channels
CMPDOS  echo prepare file for total dos ...
        echo 24 25 -1.2 0.7 | pldos -fplot -lst="1" tdos.gas
        mv dosp.dat dost.dat
        cat plot.dos | grep dosp | sed "s/-lt 1,/-lt 2,/" | sed "s/dosp/dost/" >plot.dos~
        echo prepare file for partial dos ...
        echo 24 7 -1.2 0.7 | pldos -fplot -lst="1:6" pdos.gas
        fplot -frme 0,1,0,.7 -y 0,24 -lt 1 dost.dat -lt 2,col=.5 dosp.dat
# Compare the sum of partial (sphere) dos to the total dos
TSTIDOS echo prepare file for total dos ...
        echo 24 25 -1.2 0.7 | pldos -fplot -lst="1" -int tdos.gas
        mv dosp.dat dost.dat
        cat plot.dos | grep dosp | sed "s/-lt 1,/-lt 2,/" | sed "s/dosp/dost/" >plot.dos~
        echo prepare file for partial dos ...
 	lmf  gas -vpdos=t --pdos~nl=3~mode=1 --rs=1,0 -vnit=1
        lmdos gas -vpdos=t --pdos~nl=3~mode=1 --dos:npts=1001:window=-1.2,.7
        echo 30 25 -1.2 0.7 | pldos -fplot -lst="1:12" -int dos.gas
        echo add tdos to plot
        echo "  -colsy 2 -lt 2,col=1,0,0 -qr dost.dat" >>plot.dos
        fplot -pr10 -f plot.dos
CLEAN   rm -f \{ctrl,dos,qpts,qp1,qp}.gas
